{
  "name": "javascript-lp-solver",
  "description": "Easy to use, JSON oriented Linear Programming and Mixed Int. Programming Solver",
  "version": "0.4.24",
  "private": false,
  "authors": [
    "Justin W. Wolcott <justin.w.wolcott@gmail.com>"
  ],
  "contributors": [
    "Brice Chevalier <bchevalier@wizcorp.jp>",
    "Lo√Øc Venerosy <lvenerosy@wizcorp.jp>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JWally/jsLPSolver"
  },
  "main": "./dist/src/main.js",
  "browser": {
    "./dist/src/External/main.js": "./dist/src/shims/External.js"
  },
  "types": "./types/main.d.ts",
  "devDependencies": {
    "benchmark": "*",
    "grunt": "*",
    "grunt-browserify": "*",
    "grunt-contrib-jshint": "*",
    "grunt-contrib-uglify": "*",
    "grunt-jsbeautifier": "*",
    "grunt-mocha-test": "*",
    "mocha": "*",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "compile": "tsc --allowJs false --emitDeclarationOnly --declarationDir types/generated",
    "prepublishOnly": "npm run build",
    "test": "npm run test:sanity",
    "test:sanity": "JSLP_TEST_SUITE=test-sanity mocha -r ./scripts/ts-node-register.js test/solver.problems.js",
    "test:speed": "JSLP_TEST_SUITE=test-speed mocha -r ./scripts/ts-node-register.js test/solver.problems.js -- test-speed"
  },
  "keywords": [
    "Linear",
    "Programming",
    "Integer",
    "Programming",
    "Solver",
    "Linear Programming",
    "Linear Optimization",
    "Simplex",
    "Mixed Integer Optimization",
    "Mixed Integer Programming"
  ],
  "license": "Unlicense"
}
